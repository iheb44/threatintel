{
  "dashboard": {
    "id": null,
    "title": "Dark Web Monitoring Overview",
    "tags": ["darkweb", "monitoring", "security"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total Posts Collected",
        "type": "stat",
        "targets": [
          {
            "expr": "elasticsearch_index_docs_count{index='posts'}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "lcd",
              "align": "center"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Posts Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(elasticsearch_index_docs_count{index='posts'}[5m])",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Elastalert Alerts",
        "type": "stat",
        "targets": [
          {
            "expr": "count(elasticsearch_index_docs_count{index='elastalert_status'})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Alert Types",
        "type": "piechart",
        "targets": [
          {
            "expr": "count by (alert_type) (elasticsearch_index_docs_count{index='elastalert_status'})",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "System Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job='prometheus'}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Celery Workers",
        "type": "stat",
        "targets": [
          {
            "expr": "count(celery_tasks_active{})",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 8
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 36,
    "version": 0,
    "uid": "darkweb-overview"
  }
}
